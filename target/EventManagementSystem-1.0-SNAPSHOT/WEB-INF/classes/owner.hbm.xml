<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="app.eventmanagementsystem.pojo.Owner" table="ownertable">
        
        <id name="ownerid" column="ownerid" type="long">
            <generator class="native" />
        </id>
        
        <property name="ownername" column="ownername" type="string" />
        <property name="emailid" column="emailid" type="string" unique="true"/>
        <property name="password" column="password" type="string" />
  
        
    </class>
</hibernate-mapping>
