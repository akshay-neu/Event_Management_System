<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="app.eventmanagementsystem.pojo.Venue" table="venuetable">
        
        <id name="venueid" column="venueid" type="long">
            <generator class="native" />
        </id>
        
        <property name="venuename" column="venuename" type="string" unique="true"/>
        <property name="address" column="address" type="string" />
        <property name="capacity" column="capacity" type="long" />
  
        
    </class>
</hibernate-mapping>
